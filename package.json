    "apollo-link-state": "^0.4.1",
    "apollo-link-ws": "^1.0.12",
    "autoprefixer": "7.1.6",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv-expand": "4.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.11.2",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "fs-extra": "3.0.1",
    "graphql": "^0.13.2",
    "graphql-import": "^0.6.0",
    "graphql-tag": "^2.9.2",
    "html-webpack-plugin": "2.29.0",
    "jest": "22.4.2",
    "lodash.merge": "^4.6.1",
    "merge-graphql-schemas": "^1.5.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.8.0",
    "react-apollo-hooks": "^0.2.1",
    "react-dev-utils": "^7.0.1",
    "react-dom": "^16.8.0",
    "react-paypal-button": "^1.2.2",
    "react-stripe-elements": "^2.0.1",
    "resolve": "1.6.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "22.0.1",
    "ts-loader": "^2.3.7",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.2.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "0.6.2",
    "uuid": "^3.3.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "dotenv -- node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "coverage": "jest --collectCoverageFrom=src/**.js --coverage src"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/debug": "0.0.30",
    "@types/dotenv": "^4.0.3",
    "@types/jest": "^23.1.1",
    "@types/lodash": "^4.14.111",
    "@types/prop-types": "^15.5.6",
    "@types/reach__router": "^1.2.2",
    "@types/react": "^16.7.11",
    "@types/react-dom": "^16.0.6",
    "@types/react-stripe-elements": "^1.1.9",
    "@types/uuid": "^3.4.3",
    "antd": "^3.6.4",
    "dotenv": "^6.2.0",
    "dotenv-cli": "^1.4.0",
    "graphql-faker": "^1.7.6",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "node-sass": "^4.11.0",
    "react-apollo": "^2.4.1",
    "sass-loader": "^7.0.3",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
